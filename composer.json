{
    "name": "ecohead/symfony-extra-validator-bundle",
    "type": "symfony-bundle",
    "description": "A set of constraints to enhance possibilities of validation in a Symfony project.",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "aureldvx",
            "email": "opensource@aureldvx.net"
        },
        {
            "name": "sulyaniggui",
            "email": "innovisions33@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "ext-intl": "*",
        "symfony/http-kernel": "^7.0",
        "symfony/form": "^7.0",
        "symfony/validator": "^7.0",
        "doctrine/orm": "^3.1",
        "symfony/doctrine-bridge": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Ecohead\\ExtraValidatorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ecohead\\ExtraValidatorBundle\\Tests\\": "tests/"
        }
    }
}
